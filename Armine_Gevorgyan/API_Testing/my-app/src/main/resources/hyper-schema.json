{
     "definitions": {},
     "$schema": "http://json-schema.org/draft-07/schema#",
     "$id": "http://example.com/root.json",
     "type": "object",
     "title": "The Root Schema",
     "required": [
       "name",
       "salary",
       "age",
       "id"
     ],
     "properties": {
      "name": {
           "$id": "#/properties/name",
           "type": "string",
           "title": "The Name Schema",
           "default": "",
           "examples": [
               "test"
             ],
           "pattern": "^(.*)$"
         },
       "salary": {
           "$id": "#/properties/salary",
           "type": "string",
           "title": "The Salary Schema",
           "default": "",
           "examples": [
               "123"
             ],
           "pattern": "^(.*)$"
         },
       "age": {
           "$id": "#/properties/age",
           "type": "string",
           "title": "The Age Schema",
           "default": "",
           "examples": [
               "23"
             ],
         "pattern": "^(.*)$"
       },
      "id": {
         "$id": "#/properties/id",
         "type": "string",
         "title": "The Id Schema",
         "default": "",
         "examples": [
           "719"
         ],
         "pattern": "^(.*)$"
       }
     }
  }
