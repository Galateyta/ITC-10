{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "name",
    "salary",
    "age"
  ],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "auuuoaaaaa"
      ],
      "pattern": "^(.*)$"
    },
    "salary": {
      "$id": "#/properties/salary",
      "type": "string",
      "title": "The Salary Schema",
      "default": "",
      "examples": [
        "1123"
      ],
      "pattern": "^(.*)$"
    },
    "age": {
      "$id": "#/properties/age",
      "type": "string",
      "title": "The Age Schema",
      "default": "",
      "examples": [
        "23"
      ],
      "pattern": "^(.*)$"
    }
  }
}