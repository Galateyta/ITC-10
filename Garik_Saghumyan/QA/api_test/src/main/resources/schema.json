      ],
      "pattern": "^(.*)$"
    },
    "employee_age": {
      "$id": "#/properties/employee_age",
      "type": "string",
      "title": "The Employee_age Schema",
      "default": "",
      "examples": [
        "23"
      ],
      "pattern": "^(.*)$"
    },
    "profile_image": {
      "$id": "#/properties/profile_image",
      "type": "string",
